<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="studyPdfReport"
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView"
          p:url="classpath:/jasper/study.jrxml"
          p:reportDataKey="datasource"/>

    <bean id="studyXlsReport"
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsXlsView"
          p:url="classpath:/jasper/study.jrxml"
          p:reportDataKey="datasource"/>

    <bean id="studyHtmlReport"
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsHtmlView"
          p:url="classpath:/jasper/study.jrxml"
          p:reportDataKey="datasource"/>

    <bean id="studyCsvReport"
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsCsvView"
          p:url="classpath:/jasper/study.jrxml"
          p:reportDataKey="datasource"/>

    <bean id="mahasiswaPdfReport"
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView"
          p:url="classpath:/jasper/mahasiswa.jrxml"
          p:reportDataKey="datasource"/>

    <bean id="mahasiswaXlsReport"
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsXlsView"
          p:url="classpath:/jasper/mahasiswa.jrxml"
          p:reportDataKey="datasource"/>

    <bean id="mahasiswaHtmlReport"
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsHtmlView"
          p:url="classpath:/jasper/mahasiswa.jrxml"
          p:reportDataKey="datasource"/>

    <bean id="mahasiswaCsvReport"
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsCsvView"
          p:url="classpath:/jasper/mahasiswa.jrxml"
          p:reportDataKey="datasource"/>

</beans>